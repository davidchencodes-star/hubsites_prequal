<?xml version="1.0" encoding="UTF-8"?>
<?adf version="1.0"?>
<adf>
	<prospect status="new">
		<id sequence="1" source="Get My Auto - PreQual">GMA PREQUAL"></id>
		<requestdate>{{requestDate}}</requestdate>
		<customer>
			<contact>
				<name part="first" type="individual">{{firstName}}</name>
				<name part="middle" type="individual">{{middleName}}</name>
				<name part="last" type="individual">{{lastName}}</name>
				<email>{{email}}</email>
				<phone type="cellphone">{{mobilephone}}</phone>
				<phone type="phone">{{homePhone}}</phone>
				<address type="home">
					<street>{{address}}</street>
					<city>{{city}}</city>
					<regioncode>{{state}}</regioncode>
					<postalcode>{{zip}}</postalcode>
				</address>
			</contact>
		</customer>
		<provider>
			<name part="full" type="business">Get My Auto</name>
			<service>PreQual</service>
			<url>https://www.getmyauto.com</url>
			<email>support@getmyauto.com</email>
		</provider>
	</prospect>
</adf>
